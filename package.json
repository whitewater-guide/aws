{
  "name": "aws",
  "version": "0.1.0",
  "bin": {
    "aws": "bin/aws.js"
  },
  "scripts": {
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "clean": "rm -rf ./cdk.out ./dist",
    "start": "ts-node scripts/start_stop.ts start",
    "stop": "ts-node scripts/start_stop.ts stop",
    "manual": "ts-node scripts/manual_backup_restore.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.13.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1046.0",
    "chalk": "^5.0.0",
    "commander": "^9.0.0",
    "constructs": "^10.0.70",
    "lodash": "^4.17.21",
    "p-wait-for": "4.1.0",
    "pg": "^8.7.1",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.13.0",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/aws-lambda": "^8.10.88",
    "@types/aws-sdk": "^2.7.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "17.0.20",
    "@types/pg": "^8.6.1",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "aws-cdk": "2.13.0",
    "commitlint": "^16.2.1",
    "esbuild": "0.14.23",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "utility-types": "^3.10.0"
  },
  "targets": {
    "cdk-lambda": {
      "context": "node",
      "includeNodeModules": {
        "aws-sdk": false,
        "pg-native": false
      },
      "sourceMap": false,
      "minify": false,
      "engines": {
        "node": ">= 14"
      }
    }
  }
}
